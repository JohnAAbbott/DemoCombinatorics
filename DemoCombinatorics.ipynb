   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "4-element Vector{Partition{Int64}}:\n",
       "[50, 20, 20, 5, 2, 2]\n ",
       "[50, 20, 20, 5, 2, 1, 1]\n",
       "[50, 20, 10, 10, 5, 2, 2]\n",
       "[50, 20, 10, 10, 5, 2, 1, 1]"
      ]
     },
     "execution_count": 24,
     "metadata": {},
     "output_type": "execute_result"
    }
    ],
   "source": ["collect(partitions(99, coins, [2, 2, 2, 2, 2, 2]))"]
